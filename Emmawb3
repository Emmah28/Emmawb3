    // Handle form submission animation (if needed)
    function handleSubmit(event) {
      event.preventDefault();
      const form = event.target;
      gsap.to(form, { duration: 0.5, scale: 0.95, yoyo: true, repeat: 1, ease: "power1.inOut" });
      document.getElementById('successMsg').classList.remove('hidden');
      form.reset();
    }

    // Animate floating images
    gsap.utils.toArray(".floating-image").forEach((el) => {
      gsap.to(el, {
        y: "-=200px",
        duration: 30,
        repeat: -1,
        ease: "linear",
        delay: Math.random() * 5
      });
    });

    // Animate Lottie elements
    gsap.utils.toArray(".floating-animation").forEach((el) => {
      gsap.to(el, {
        y: "-=150px",
        duration: 40,
        repeat: -1,
        ease: "linear",
        delay: Math.random() * 5
      });
    });

    // Initialize tsParticles
    tsParticles.load("tsparticles", {
      particles: {
        color: { value: "#10b981" },
        links: { enable: true, color: "#10b981" },
        move: { enable: true, speed: 1 },
        size: { value: 3 },
        number: { value: 50 }
      },
      interactivity: {
        events: {
          onhover: { enable: true, mode: "repulse" },
          onclick: { enable: true, mode: "push" }
        }
      }
    });
  </script></body>
